body {
  padding-left: 10px;
}

/* DataTables */
table.dataTable tr.odd.row_selected td {
    background-color: #9FAFD1;
}
table.dataTable tr.even.row_selected td {
    background-color: #9FAFD1;
}

.notimplemented {
    text-decoration:line-through;
}

div.collapseLegend a {
    text-decoration: none;
    color: black;
}
div.collapseLegend a legend:after {
    font-family:'Glyphicons Halflings';
    content:"\e114";
    float: right;
    color: grey;
}
div.collapseLegend a.collapsed legend:after {
    font-family:'Glyphicons Halflings';
    content:"\e080";
    float: right;
    color: grey;
}
